    "wait-on": "^3.3.0",
    "webpack": "^4.40.0",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "xhr-mock": "^2.5.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.18.1",
    "@concord-consortium/jsxgraph": "^0.99.8-cc.1",
    "ag-grid-community": "^21.2.1",
    "ag-grid-react": "^21.2.1",
    "classnames": "^2.2.6",
    "concord-react-components": "^0.1.0",
    "expr-eval": "^1.2.3",
    "file-saver": "^2.0.2",
    "firebase": "^6.6.1",
    "immutable": "^3.8.2",
    "initials": "^3.0.1",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.15",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "mobx-state-tree": "^3.14.1",
    "nanoid": "^2.1.1",
    "query-string": "^6.8.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-dom-factories": "^1.0.2",
    "react-sizeme": "^2.6.7",
    "slate": "^0.47.8",
    "slate-html-serializer": "^0.8.10",
    "slate-md-serializer": "^5.4.3-1",
