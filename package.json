    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nock": "^10.0.6",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prompt-confirm": "^2.0.4",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "script-loader": "^0.7.2",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.7.2",
