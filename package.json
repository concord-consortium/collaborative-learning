{
  "name": "collaborative-learning",
  "version": "0.6.0",
  "description": "Collaborative Learning environment",
  "main": "index.js",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/utilities/test-utils.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot --content-base dist/",
    "build": "npm-run-all lint:build clean build:webpack",
    "build:webpack": "cross-env NODE_OPTIONS=--max_old_space_size=4096 webpack --mode production",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:build": "tslint -c tslint-build.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint -c tslint-build.json --fix 'src/**/*.{ts,tsx}'",
    "lint:unused": "tsc --noUnusedLocals --project .",
    "migrate": "node ./migrations/migrate",
    "migrate:debug": "node --inspect-brk ./migrations/migrate",
    "test": "jest",
    "test:all": "npm-run-all build test start",
    "test:coverage": "jest --coverage",
    "test:debug": "node --nolazy --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache",
    "test:watch": "jest --watchAll",
    "test:coverage:watch": "jest --coverage --watchAll",
    "test:cypress": "cypress run --config video=false,defaultCommandTimeout=8000 --env testEnv=local",
    "test:cypress:open": "cypress open --env testEnv=local",
    "test:full": "npm-run-all test test:cypress",
    "test:production": "cypress open --env testEnv=production",
    "test:master": "cypress open --env testEnv=master ",
    "test:branch": "cypress open --env testEnv=branch",
    "test:local": "cypress open --env testEnv=local"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/collaborative-learning.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/collaborative-learning/issues"
  },
  "homepage": "https://github.com/concord-consortium/collaborative-learning#readme",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.0.2",
    "@firebase/app-types": "^0.3.9",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/file-saver": "^2.0.0",
    "@types/jest": "^24.0.11",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.123",
    "@types/nanoid": "^1.2.1",
    "@types/nock": "^9.3.1",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/slate": "^0.43.9",
    "@types/slate-plain-serializer": "^0.6.1",
    "@types/slate-react": "0.20.3",
    "@types/superagent": "^3.8.7",
    "@types/uuid": "^3.4.4",
    "autoprefixer": "^9.5.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.7.1",
    "copy-webpack-plugin": "^5.0.2",
