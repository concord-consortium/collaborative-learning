{
  "name": "collaborative-learning",
  "version": "1.1.0",
  "description": "Collaborative Learning environment",
  "main": "index.js",
  "config": {
    "pageTitle": "CLUE",
    "pageDescription": "Collaborative Learning User Environment",
    "branchTagRegEx": "/collaborative-learning\\.concord\\.org\\/(branch|tag)(\\/|$)/i",
    "masterBranchRegEx": "/collaborative-learning\\.concord\\.org\\/branch\\/master(\\/|$)/i",
    "rollbarAccessToken": "fb9dd070b7344cc7a5e529d8fa25e765"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/utilities/test-utils.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot --content-base dist/",
    "build": "npm-run-all lint:build clean build:webpack",
    "build:debug": "npm-run-all lint clean build:webpack",
    "build:webpack": "cross-env NODE_OPTIONS=--max_old_space_size=4096 webpack --mode production",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:build": "tslint -c tslint-build.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint -c tslint-build.json --fix 'src/**/*.{ts,tsx}'",
    "lint:unused": "tsc --noUnusedLocals --project .",
    "migrate": "node ./migrations/migrate",
    "migrate:debug": "node --inspect-brk ./migrations/migrate",
    "test": "jest",
    "test:all": "npm-run-all build test start",
    "test:coverage": "jest --coverage",
    "test:debug": "node --nolazy --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache",
    "test:watch": "jest --watchAll",
    "test:coverage:watch": "jest --coverage --watchAll",
    "test:cypress": "cypress run --config video=false,defaultCommandTimeout=10000 --env testEnv=local --record --parallel",
    "test:cypress:smoke": "cypress run --spec 'cypress/integration/clue/smoke/single_student_canvas_test.js' --config video=false,defaultCommandTimeout=10000 --env testEnv=local",
    "test:cypress:branch": "cypress run --spec 'cypress/integration/clue/branch/**/*' --config video=false,defaultCommandTimeout=10000 --env testEnv=local --record --parallel",
    "test:full": "npm-run-all test test:cypress",
    "test:production": "cypress open --env testEnv=production",
    "test:master": "cypress open --env testEnv=master ",
    "test:branch": "cypress open --env testEnv=branch",
    "test:local": "cypress open --env testEnv=local"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/collaborative-learning.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/collaborative-learning/issues"
  },
  "homepage": "https://github.com/concord-consortium/collaborative-learning#readme",
