        {"id": "kEZwo8OpANVqVmLL", "content": {"type": "Table", "name": "Table 1", "columns": [{"name": "d", "width": 80, "values": [0, 1, 2, 3, 4, 20, 25, 30]}, {"name": "A", "width": 80, "values": [5, 5.5, 6, 6.5, 7, 15, 17.5, 20]}]}}
