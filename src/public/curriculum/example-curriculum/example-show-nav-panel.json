        }
      ]
    },
    "toolbar": [{"id": "Text", "title": "Text", "isTileTool": true}, {"id": "Table", "title": "Table", "isTileTool": true}, {"id": "Drawing", "title": "Drawing", "isTileTool": true}, {"id": "delete", "title": "Delete", "iconId": "icon-delete-tool", "isTileTool": false}],
    "stamps": []
