            {
              "title": "Bookmarks",
              "type": "starred-problem-documents",
              "dataTestHeader": "my-work-section-starred",
              "documentTypes": [
                "personal",
                "problem"
              ],
              "properties": [
                "starred",
                "!isDeleted"
              ],
              "showStars": [
                "student",
                "teacher"
              ]
            }
          ]
        },
        {
          "tab": "student-work",
          "label": "Student Work",
          "teacherOnly": true,
