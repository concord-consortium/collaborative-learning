{
  "code": "qa-config-subtabs",
  "abbrevTitle": "QA2",
  "title": "QA Curriculum Configured SubTabs",
  "subtitle": "Some Subtabs",
  "placeholderText": "Type in me!",
  "config": {
    "placeholderText": "This is where users can type text",
    "defaultDocumentType": "problem",
    "initiallyHideExemplars": true,
    "settings": {
      "ai": {
        "systemPrompt": "You are a helpful, collaborative student, who always talks like a pirate."
      },
      "table": {
        "numFormat": ".2~f"
      },
      "datacard": {
        "tools": [
          "duplicate",
          "merge-in",
          [
            "data-set-view",
            "Table"
          ],
          [
            "data-set-view",
            "graph"
          ],
          "image-upload",
          "delete-attribute"
        ]
      },
      "graph": {
        "defaultAxisLabels": {
          "bottom": "time",
          "left": "dist"
        },
        "emptyPlotIsNumeric": true,
        "scalePlotOnValueChange": true,
        "defaultSeriesLegend": true,
        "connectPointsByDefault": true,
        "tools": [
          "link-tile-multiple",
          "add-points-by-hand",
          "|",
          "fit-all",
          "toggle-lock",
          "movable-line",
          "|",
          "move-points",
          "add-points",
          "delete"
        ]
      },
      "diagram": {
        "maxTiles": 1
      },
      "simulator": {
        "defaultSimulation": "terrarium",
        "maxTiles": 1
      }
    },
    "enableCommentRoles": ["student", "teacher", "researcher"],
    "showCommentTag": true,
    "commentTags": {
      "diverging": "Diverging Designs",
      "converging": "Converging Designs",
      "materials": "Varies Material/Surface",
      "physical": "Varies Physical Factors",
      "user": "Varies Human Factors",
      "system": "Varies System Factors",
      "unit-rate": "Unit Rate"
    },
    "tagPrompt": "Identify Design Approach",
    "tools": ["Simulator"],
    "navTabs": {
      "lazyLoadTabContents": true,
      "showNavPanel": true,
      "tabSpecs": [
        {
          "tab": "problems",
          "label": "Problems",
          "sections": [
            {
              "initials": "REF",
              "title": "Reference",
              "type": "problem-documents"
            },
            {
              "initials": "PR",
              "title": "Practice",
              "type": "problem-documents"
            },
            {
              "initials": "EX",
              "title": "Experiment",
              "type": "problem-documents"
            }
          ]
        },
        {
          "tab": "sort-work",
          "label": "Sort Work",
